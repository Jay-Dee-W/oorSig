{
  "name": "project-oorsig",
  "version": "1.0.0",
  "description": "gitstart github project",
  "main": "index.js",
  "scripts": {
    "postinstall": "cp .env.example .env",
    "predev": "npm run relay",
    "dev": "next dev",
    "prebuild": "npm run relay",
    "build": "next build",
    "start": "next start",
    "prerelay": "npm run schema:merge && mkdir -p src/relay/__generated__",
    "relay": "relay-compiler",
    "postrelay": "npm run schema:generateOperationTypes",
    "premock:nobackend": "npm run schema:merge",
    "mock:nobackend": "npx auto-graphql-faker src/relay/final.graphql",
    "mock:local": "npx auto-graphql-faker src/relay/extend.graphql --extend http://localhost:4000/developer/graphql --forward-headers accept Authorization",
    "mock:dev": "npx auto-graphql-faker src/relay/extend.graphql --extend https://api.gitstart.dev/developer/graphql --forward-headers accept Authorization",
    "pretest": "npm run relay",
    "test": "vitest",
    "lint": "next lint",
    "format": "prettier --write . --ignore-path .prettierignore",
    "format:check": "prettier --check . --ignore-path .prettierignore",
    "schema:merge": "ts-node -T ./scripts/mergeSchemas.ts && prettier --write ./src/relay/final.graphql",
    "schema:generateOperationTypes": "ts-node -T ./scripts/generateOperationTypes.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "author": "JD Wilson",
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@heroicons/react": "^1.0.6",
    "@mdx-js/loader": "^2.1.2",
    "@next/bundle-analyzer": "^12.2.2",
    "@next/mdx": "^12.2.2",
    "@xstyled/emotion": "^3.6.0",
    "dompurify": "^2.3.8",
    "downshift": "^7.6.0",
    "es-cookie": "^1.3.2",
    "next": "^12.2.2",
    "next-compose-plugins": "^2.2.1",
    "next-svgr": "^0.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-hook-form": "^7.33.1",
    "react-markdown": "^8.0.3",
    "react-relay": "^14.0.0",
    "react-relay-network-modern": "^6.2.1",
    "regenerator-runtime": "^0.13.9",
    "rehype-raw": "^6.1.1",
    "relay-runtime": "^14.0.0",
    "remark-gfm": "^3.0.1"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.2",
    "@storybook/addon-interactions": "^7.0.2",
    "@storybook/addon-links": "^7.0.2",
    "@storybook/blocks": "^7.0.2",
    "@storybook/nextjs": "^7.0.2",
    "@storybook/react": "^7.0.2",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@svgr/webpack": "^7.0.0",
    "@testing-library/react": "^13.3.0",
    "@types/cookie": "^0.5.1",
    "@types/dompurify": "^2.3.3",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/react-relay": "^13.0.2",
    "@vitejs/plugin-react": "^2.0.0",
    "auto-graphql-faker": "^1.1.1",
    "better-graphql": "^16.7.2",
    "eslint": "^8.18.0",
    "eslint-config-next": "^12.1.6",
    "eslint-plugin-storybook": "^0.6.11",
    "jsdom": "^20.0.0",
    "prettier": "^2.7.1",
    "relay-compiler": "^14.0.0",
    "relay-config": "^12.0.1",
    "storybook": "^7.0.2",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4",
    "vite-plugin-checker": "^0.4.9",
    "vite-tsconfig-paths": "^3.5.0",
    "vitest": "^0.18.1"
  }
}
